{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently \"pnpm run dev:db\" \"nuxt dev\"",
    "generate": "nuxt generate",
    "preview": "npx nuxthub preview",
    "deploy": "npx nuxthub deploy",
    "postinstall": "nuxt prepare",
    "lint": "ultracite format",
    "lint:fix": "biome check --fix --unsafe",
    "dev:db": "turso dev --db-file local.db",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@libsql/client": "^0.15.4",
    "@nuxt/icon": "1.12.0",
    "@nuxthub/core": "^0.8.25",
    "@nuxtjs/color-mode": "3.5.2",
    "@tailwindcss/vite": "^4.1.5",
    "better-auth": "^1.2.7",
    "concurrently": "^9.1.2",
    "drizzle-orm": "^0.43.1",
    "nuxt": "^3.16.2",
    "tailwindcss": "^4.1.5",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@iconify-json/tabler": "^1.2.17",
    "daisyui": "^5.0.35",
    "drizzle-kit": "^0.31.1",
    "typescript": "^5.8.3",
    "ultracite": "^4.2.1",
    "vue-tsc": "^2.2.10",
    "wrangler": "^4.12.1"
  },
  "packageManager": "pnpm@10.9.0",
  "pnpm": {
    "onlyBuiltDependencies": ["@parcel/watcher", "esbuild", "sharp", "workerd"]
  }
}
